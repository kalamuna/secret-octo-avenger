                  td(colspan='3') Collapsed to start, horizontal above breakpoints
                tr
                  th.text-nowrap Container width
                  td None (auto)
                  td 750px
                  td 970px
                  td 1170px
                tr
                  th.text-nowrap Class prefix
                  td
                    code .col-xs-
                  td
                    code .col-sm-
                  td
                    code .col-md-
                  td
                    code .col-lg-
                tr
                  th.text-nowrap # of columns
                  td(colspan='4') 12
                tr
                  th.text-nowrap Column width
                  td.text-muted Auto
                  td ~62px
                  td ~81px
                  td ~97px
                tr
